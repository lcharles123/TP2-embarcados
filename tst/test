read A0 ;entrada da funcao fatorial, usar <= 7

move A0 A2 ;guardar a condicao de parada
fat:
load_c A1 1
push A0             ; empilha a entrada
subtract A0 A1  ; 
subtract A1 A0
jmpn A1 fat

load_c A1 1
mul:
pop A0              ; desempilha e multiplica
multiply A1 A0
subtract A0 A2 ; sair caso chegou ao ultimo numero
jmpz A0 sai
jump mul
sai: 
write A1
stop